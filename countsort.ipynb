    "def countingSort(arr):\n",
    "    # count sort for 0~99\n",
    "    out=[0]*100\n",
    "    for i in arr:\n",
    "        out[i]+=1\n",
    "    for i in range(100):\n",
    "        output=[]\n",
    "        if out[i]!=0:\n",
    "            output+=[i]*out[i]\n",
    "    return output"
   ]
  }
